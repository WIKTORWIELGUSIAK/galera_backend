{
  "version": 2,
  "name": "galera-api",
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node",
      "config": {
        "env": {
          "PORT": "3001"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
